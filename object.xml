<?xml version="1.0" encoding = "UTF-8"?>

<!-- 页面对象配置文档 
     object所有子节点中：一级节点名称页面简称。二级节点名称为页面具体对象类型简称
     页面对象节点，type为对象识别属性，value为识别属性的具体指
      -->
      
<对象>

     <首页>  
         <搜索分类> <type>id</type>   <value>category</value> </搜索分类>         
         <关键词> <type>id</type>    <value>keyword</value> </关键词>  
         <搜索> <type>name</type>    <value>imageField</value> </搜索>         
         <留言板> <type>linkText</type>    <value>留言板</value> </留言板>   
         <退出登录> <type>xpath</type>      <value>//a[@href='user.php?act=logout']</value> </退出登录>
         <精品推荐-价格> <type>xpath</type>   <value>//div[@id='itemBest']/h2/a</value> </精品推荐-价格>
         <精品推荐-产品图片> <type>xpath</type>      <value>//div[@id='show_best_area']/div[@class='goodsItem']/a/img</value> </精品推荐-产品图片>
     </首页>
     
     <登录页>
         <用户名> <type>name</type>   <value>username</value> </用户名>
         <密码> <type>name</type>    <value>password</value> </密码>
         <立即登录> <type>name</type>     <value>submit</value> </立即登录>
     </登录页>
     
     <留言板>
         <点击邮件地址> <type>name</type>    <value>user_email</value> </点击邮件地址>
         <留言类型-投诉> <type>xpath</type>      <value>//input[@name='msg_type' and @value='1']</value> </留言类型-投诉>
         <主题> <type>name</type>      <value>msg_title</value> </主题>
         <留言内容> <type>name</type>     <value>msg_content</value> </留言内容>
         <我要留言> <type>className</type>     <value>bnt_blue_1</value> </我要留言>
         <成功提示> <type>xpath</type>    <value>//div[@class='boxCenterList RelaArticle']//p</value> </成功提示>
     </留言板>
</对象>